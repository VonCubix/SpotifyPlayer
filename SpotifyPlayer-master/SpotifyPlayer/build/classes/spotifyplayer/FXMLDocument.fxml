      <Label fx:id="labelDenominator" layoutX="584.0" layoutY="391.0" text="/0:30" />
      <Label fx:id="labelSongTime" alignment="CENTER_RIGHT" layoutX="556.0" layoutY="391.0" text="Label" textAlignment="RIGHT" />
